{
    "presets": ["next/babel"],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ],
        [
            "inline-react-svg",
            {
                "svgo": {
                    "plugins": [
                        {
                            "removeAttrs": { "attrs": "(data-name)" }
                        },
                        {
                            "cleanupIDs": true
                        }
                    ]
                }
            }
        ]
    ]
}