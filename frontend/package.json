{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "bin": "index.js",
  "scripts": {
    "dev": "API_BASE_URL=http://localhost:4000/graphql node index.js",
    "build": "API_BASE_URL=https://api.simpletwitter.tk/graphql next build",
    "telemetry_status": "next telemetry status",
    "telemetry_disable": "next telemetry disable",
    "analyze": "ANALYZE=true next build",
    "start": "NODE_ENV=production node index.js",
    "fetch-pkg": "pkg-fetch -n node10 -p alpine -a x64",
    "dist": "pkg . --target host --out-path dist"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.1.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.24.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.12.2",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "less": "^3.10.3",
    "less-vars-to-js": "^1.3.0",
    "moment": "^2.24.0",
    "next": "9.1.1",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^1.1.3",
    "next-images": "^1.2.0",
    "next-redux-saga": "^4.0.2",
    "next-redux-wrapper": "^4.0.1",
    "node-sass": "^4.12.0",
    "null-loader": "^3.0.0",
    "react": "16.10.2",
    "react-contenteditable": "^3.3.3",
    "react-dom": "16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "reduxsauce": "^1.1.0"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^1.1.1",
    "pkg": "^4.4.0",
    "pkg-fetch": "^2.6.2"
  },
  "pkg": {
    "assets": [
      ".next/**/*",
      "public/**/*"
    ],
    "scripts": [
      ".next/server/**/*.js"
    ]
  }
